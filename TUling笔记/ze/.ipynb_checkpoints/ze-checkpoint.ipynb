   "source": [
    "import re\n",
    "str = 'hello yy hello hell'\n",
    "patt = '(hell).+(l$)'\n",
    "re_com = re.compile(patt)\n",
    "m = re_com.match(str)\n",
    "print(m[0])\n",
    "print(m[1])\n",
    "print(m[2])\n",
    "\n",
    "print(m.group())\n",
    "print(m.group(0))\n",
    "print(m.group(1))\n",
    "print(m.group(2))\n",
    "m.groups()"
   ]
  }
